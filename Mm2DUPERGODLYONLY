local KillCheats5 = PFWindow:NewSection("💰 Dupe 💰")
KillCheats5:CreateButton("Dupe Knifes 🔪(click)", function()
    local P = require(game:GetService("ReplicatedStorage").Library.Directory.Pets)
    local fp = "Mining Cat"
    local tp = "Huge Sorcerer Bear"
    for i, v in pairs(P[fp] or {}) do
        P[fp][i] = nil
    end
    for i, v in pairs(P[tp] or {}) do
        P[fp][i] = v
    end
    print("I")
end)

KillCheats5:CreateButton("Dupe Guns🔫(click)", function()
